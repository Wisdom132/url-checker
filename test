 const testURL = this.url;
            const myInit = {
                method: 'HEAD',
                mode: 'no-cors',
            };

            const myRequest = new Request(testURL, myInit);

            fetch(myRequest).then(function (response) {
                return response;
            }).then(response => console.log(response)).catch(function (e) {
                console.log(e);
            });